<% if artists && artists.length > 0 %>
	<% artist_names = artists.keys.sort { |x,y| artists[y].rank <=> artists[x].rank } %>
	<% max_rank = artists[artist_names.first].rank %>
	<% min_rank = artists[artist_names.last].rank %>
	<% for i in (0..artist_names.length-1) %><%= rank_size_artist(artists[artist_names[i]], max_rank, min_rank, artists[artist_names[i]].rank, i) %><% end-%>
<% end-%>
